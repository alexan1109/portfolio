
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    String     @id 
  email String  @unique
  name  String
  role String
}

model Projects {
  id    String     @id
  title String
  company  String
  description String
   website String
    userId String
    email String
    createdAt DateTime @default(now())
}